pipeline {
    agent {
        label 'maven'
    }

    stages {
        stage('build') {
            steps {
                script {
                    echo "Building the project..."
                    sh 'pwd'
                    sh 'ls -la'
                    sh 'echo "Building the project..."'
                    sh 'mvn --version'
                    sh 'mvn clean package'
                }
            }
        }
    }
}
